{
  "name": "manifold-test",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "compile": "node scripts/compile-manifold.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:lib": "vitest run tests/lib/",
    "test:services": "vitest run tests/services/",
    "test:ui-layer": "vitest run tests/ui/",
    "test:pipeline": "vitest run tests/pipeline/",
    "pipeline": "node scripts/run-pipeline-v3.js",
    "pipeline-v2": "node scripts/compile-manifold.js --manifold-only && node scripts/pipeline-v2.js",
    "pipeline-v3": "node scripts/run-pipeline-v3.js"
  },
  "devDependencies": {
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^1.0.0",
    "happy-dom": "^12.0.0",
    "@vitest/ui": "^1.0.0"
  },
  "dependencies": {
    "@gltf-transform/core": "^4.1.3",
    "@google/model-viewer": "^3.3.0",
    "@preact/signals": "^2.0.4",
    "manifold-3d": "^3.1.0",
    "tweakpane": "^4.0.5"
  }
}
