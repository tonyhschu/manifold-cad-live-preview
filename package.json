{
  "name": "manifold-test",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "compile": "node scripts/compile-manifold.js",
    "test": "npm run compile && npm run test:all",
    "test:all": "node --test tests/simple-service.test.js tests/manifold-integration.test.js tests/lib/export.test.js tests/parametric-config.test.js tests/parameter-manager.test.js",
    "test:lib": "npm run compile && node --test tests/lib/export.test.js",
    "test:services": "npm run compile && node --test tests/simple-service.test.js",
    "test:parametric": "npm run compile && node --test tests/parametric-config.test.js tests/parameter-manager.test.js",
    "test:ui": "npm run compile && node --test tests/ui/store.test.js",
    "test:integration": "npm run compile && node --test tests/manifold-integration.test.js",
    "test:skip-compile": "node --test tests/**/*.test.js",
    "test:watch": "npm run compile && node --test --watch tests/**/*.test.js",
    "pipeline": "node scripts/compile-manifold.js --manifold-only && node scripts/pipeline.js"
  },
  "devDependencies": {
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  },
  "dependencies": {
    "@gltf-transform/core": "^4.1.3",
    "@google/model-viewer": "^3.3.0",
    "@preact/signals": "^2.0.4",
    "manifold-3d": "^3.1.0",
    "tweakpane": "^4.0.5"
  }
}
